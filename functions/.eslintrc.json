{
    "env": {
      "es6": true,
      "node": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "max-len": ["error", { "code": 120 }],
      "object-curly-spacing": ["error", "never"],
      "quotes": ["error", "double"],
      "indent": ["error", 2],
      "eol-last": ["error", "always"]
    }
  }